{
  "name": "everywhere",
  "version": "0.0.1",
  "main": "electron/index.js",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "web": "node -r @babel/register internals/scripts/start.js",
    "build": "node scripts/build.js",
    "test:web": "node scripts/test.js --env=jsdom",
    "electron": "cross-env-shell NODE_ENV=development \"node -r @babel/register internals/scripts/start.js & electron -r @babel/register ./electron\"",
    "haul": "haul --config internals/webpack/webpack.native.js",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "postinstall": "yarn run flow-typed"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "electron-debug": "^1.5.0",
    "fs-extra": "5.0.0",
    "history": "^4.7.2",
    "localforage": "^1.7.1",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "promise-map-es6": "^1.3.2",
    "query-string": "^6.0.0",
    "raf": "^3.4.0",
    "re-reselect": "^2.0.0",
    "react": "^16.3.2",
    "react-art": "^16.3.2",
    "react-dev-tools": "^0.0.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-native": "^0.55.3",
    "react-native-web": "^0.6.1",
    "react-native-windows": "^0.55.0-rc.0",
    "react-redux": "^5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-native": "4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "reducer-generator-array-map": "^1.1.2",
    "reducer-generator-object-map": "^1.1.3",
    "reducer-generator-reducer": "^1.1.0",
    "reducer-generator-wildcard": "^1.1.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "redux-thunk": "2.2.0",
    "reselect": "^3.0.1",
    "set-queue": "^1.0.6",
    "setimmediate": "^1.0.5",
    "shortid": "^2.2.8",
    "source-map-support": "^0.5.5",
    "to-redux-type": "^1.2.3",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/plugin-codemod-optional-catch-binding": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/polyfill": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-flow": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-0": "^7.0.0-beta.46",
    "@babel/register": "^7.0.0-beta.46",
    "@pixi/jsdoc-template": "^2.4.0",
    "app-root-dir": "^1.0.2",
    "autoprefixer": "8.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-native-web": "^0.6.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "clean-webpack-plugin": "^0.1.19",
    "component-webpack-resolver-plugin": "^2.0.2",
    "cross-env": "^5.1.4",
    "css-loader": "0.28.11",
    "devtron": "^1.4.0",
    "electron": "^1.8.6",
    "electron-builder": "^20.10.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.11",
    "flow-bin": "^0.71.0",
    "flow-typed": "^2.4.0",
    "haul": "^1.0.0-rc.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "20.0.4",
    "lodash-webpack-plugin": "^0.11.5",
    "npm-run-all": "^4.1.2",
    "optimize-js-plugin": "^0.0.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-import": "^11.1.0",
    "postcss-inline-media": "^1.0.1",
    "postcss-loader": "^2.1.4",
    "react-scripts": "^1.1.4",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglify-js": "^3.3.22",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-dev-server": "^3.1.3",
    "webpack-manifest-plugin": "^2.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
